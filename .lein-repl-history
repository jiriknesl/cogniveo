(def state [
{:user {:id "tillda"}},
{:user {:id "jirka"}},
])
state
(map #(%) steate)
(map #(%) state)
(map #(%1) state)
(map #(%) state)
(map (fn [row] row) state)
(map (fn [row] (row :user)) state)
(map (fn [row] ((row :user) :id)) state)
(map (fn [row] (= "tillda" ((row :user) :id))) state)
(filter (fn [row] (= "tillda" ((row :user) :id))) state)
